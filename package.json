{
  "name": "proxy-text",
  "version": "1.0.0",
  "description": "Proxy server with static assets",
  "main": "server.js",
  "scripts": {
    "build": "cd assets && npm install && npm run build",
    "start": "node server.js",
    "dev": "concurrently \"cd assets && npm run dev\" \"nodemon server.js\""
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
    